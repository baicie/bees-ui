  },
  "description": "copy from antd",
  "devDependencies": {
    "@rc-component/father-plugin": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.0.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@umijs/fabric": "^3.0.0",
    "cross-env": "^7.0.2",
    "dumi": "^2.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^7.0.0",
    "father": "^4.0.0",
    "glob": "^10.0.0",
    "less": "^3.10.3",
    "np": "^7.0.0",
    "prettier": "^2.0.5",
    "rc-test": "^7.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^5.0.0"
  },
  "files": [
    "es",
    "lib",
    "types"
  ],
  "homepage": "https://react-component.github.io/overflow",
  "keywords": [
    "react",
    "react-component",
    "react-overflow",
    "overflow",
    "antd",
    "ant-design"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "name": "@bees-ui/overflow",
  "peerDependencies": {
    "preact": "^10.24.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/overflow.git"
  },
  "scripts": {
    "build": "pnpm bee build -d",
    "clean": "pnpm rimraf node_modules .turbo dist",
    "compile": "father build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "dev": "pnpm bee build --watch -d",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "lint": "eslint src/ --ext .tsx,.ts",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "now-build": "npm run docs:build",
    "prepublishOnly": "npm run compile && np --no-cleanup --yolo --no-publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "start": "dumi dev",
    "test": "rc-test",
    "test:coverage": "rc-test --coverage"
  },
  "tnpm": {
    "mode": "npm"
  },
  "types": "./types/index",
  "version": "0.0.0"
}