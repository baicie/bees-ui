  },
  "files": [
    "es",
    "lib",
    "types"
  ],
  "homepage": "http://github.com/react-component/switch",
  "keywords": [
    "react",
    "react-component",
    "react-switch",
    "switch"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write"
    ]
  },
  "main": "./lib/index",
  "module": "./es/index",
  "name": "@bees-ui/switch",
