    "cheerio": "1.0.0-rc.12",
    "dumi": "^2.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "father": "^4.0.0",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^15.1.0",
    "np": "^9.0.0",
    "prettier": "^3.1.0",
