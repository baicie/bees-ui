{
  "bugs": {
    "url": "http://github.com/react-component/collapse/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "@bees-ui/motion": "workspace:^",
    "@bees-ui/util": "workspace:^",
    "classnames": "2.x"
  },
  "description": "copy from antd",
  "devDependencies": {
    "@rc-component/father-plugin": "^1.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/fabric": "^4.0.0",
    "dumi": "^2.1.1",
    "eslint": "^8.55.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "father": "^4.1.3",
    "husky": "^9.0.0",
    "jest": "^29.1.2",
    "less": "^4.2.0",
    "lint-staged": "^15.0.2",
    "np": "^9.1.0",
    "prettier": "^3.0.3",
    "rc-test": "^7.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "es",
    "lib",
    "types"
  ],
  "homepage": "http://github.com/react-component/collapse",
  "keywords": [
    "react",
    "react-component",
    "react-rc-collapse",
    "rc-collapse",
    "collapse",
    "accordion"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,md}": "npm run prettier"
  },
  "main": "./lib/index",
  "module": "./es/index",
  "name": "@bees-ui/collapse",
  "peerDependencies": {
    "preact": "^10.24.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/collapse.git"
  },
  "scripts": {
    "build": "pnpm bee build -d",
    "clean": "pnpm rimraf node_modules .turbo dist",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "coverage": "rc-test --coverage",
    "dev": "pnpm bee build --watch -d",
    "docs:build": "dumi build",
    "docs:deploy": "npm run docs:build && gh-pages -d .doc",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "now-build": "npm run docs:build",
    "postpublish": "npm run docs:deploy",
    "prepublishOnly": "npm run compile && np --yolo --no-publish",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "dumi dev",
    "test": "rc-test"
  },
  "types": "./types/index",
  "typings": "es/index.d.ts",
  "version": "0.0.0"
}