    "@umijs/fabric": "^3.0.0",
    "cheerio": "1.0.0-rc.12",
    "cross-env": "^7.0.0",
    "dumi": "^2.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "father": "^4.0.0",
    "glob": "^8.0.1",
    "np": "^10.0.1",
    "prettier": "^3.1.1",
    "rc-test": "^7.0.15",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
