  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "@bees-ui/util": "workspace:^",
    "classnames": "^2.2.5"
  },
  "description": "copy from antd",
  "devDependencies": {
    "@rc-component/father-plugin": "^1.0.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^29.5.1",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@umijs/fabric": "^3.0.0",
    "cheerio": "1.0.0-rc.12",
    "cross-env": "^7.0.0",
    "dumi": "^2.1.2",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^7.1.0",
    "father": "^4.0.0",
    "gh-pages": "^3.1.0",
    "less": "^3.0.0",
    "np": "^7.0.0",
    "rc-test": "^7.0.15",
    "rc-tooltip": "^5.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=8.x"
  },
  "files": [
    "es",
    "lib",
    "types"
  ],
  "homepage": "https://github.com/react-component/rate",
  "keywords": [
    "react",
    "react-component",
    "react-rate",
    "rate"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "name": "@bees-ui/rate",
  "peerDependencies": {
    "preact": "^10.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-component/rate.git"
  },
  "scripts": {
    "build": "pnpm bee build --minify --sourcemap -d",
    "clean": "pnpm rimraf node_modules .turbo dist",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "coverage": "rc-test --coverage",
    "dev": "pnpm bee build --watch --sourcemap",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d .doc",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "now-build": "npm run docs:build",
    "postpublish": "npm run docs:build && npm run docs:deploy",
    "prepare": "dumi setup",
    "prepublishOnly": "npm run compile && np --yolo --no-publish",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "dumi dev",
    "test": "rc-test"
  },
  "types": "./types/index",
  "version": "0.0.0"
}