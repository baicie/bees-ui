  },
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@bees-ui/util": "workspace:^",
    "classnames": "^2.3.2"
  },
  "description": "copy from antd",
  "devDependencies": {
    "@rc-component/father-plugin": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^20.9.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/fabric": "^3.0.0",
    "cheerio": "1.0.0-rc.12",
    "dumi": "^2.0.0",
    "eslint": "^7.18.0",
    "father": "^4.0.0",
    "gh-pages": "^3.1.0",
    "glob": "^10.0.0",
    "np": "^5.0.3",
    "prettier": "^2.1.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "umi-test": "^1.9.7"
  },
  "engines": {
    "node": ">=8.x"
  },
  "files": [
    "es",
    "lib",
    "types"
  ],
  "homepage": "https://github.com/react-component/portal",
  "keywords": [
    "react",
    "react-component",
    "react-portal"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "name": "@bees-ui/portal",
  "peerDependencies": {
    "preact": "^10.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-component/portal.git"
  },
  "scripts": {
    "build": "pnpm bee build --minify --sourcemap -d",
    "clean": "pnpm rimraf node_modules .turbo dist",
    "compile": "father build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "dev": "pnpm bee build --watch --sourcemap",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "lint": "eslint src/ --ext .tsx,.ts",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "now-build": "npm run docs:build",
    "prepublishOnly": "npm run compile && np --no-cleanup --yolo --no-publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "start": "dumi dev",
    "test": "npm run test:server && npm run test:coverage",
    "test:client": "umi-test --testPathIgnorePatterns=ssr.test.tsx --testPathIgnorePatterns=ssr.test.tsx",
    "test:coverage": "npm run test:client -- --coverage",
    "test:server": "umi-test --env=node -- tests/ssr.test.tsx",
    "watch": "father dev"
  },
  "types": "./types/index",
  "typings": "es/index.d.ts",
  "version": "0.0.0"
}