{
  "bugs": {
    "url": "http://github.com/react-component/pagination/issues"
  },
  "cnpm": {
    "mode": "npm"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "@bees-ui/util": "workspace:^",
    "classnames": "^2.3.2"
  },
  "description": "copy from antd",
  "devDependencies": {
    "@rc-component/father-plugin": "^1.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^15.0.4",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/fabric": "^4.0.1",
    "coveralls": "^3.0.6",
    "cross-env": "^7.0.0",
    "dumi": "^2.1.2",
    "eslint": "^8.54.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "father": "^4.0.0",
    "gh-pages": "^6.1.0",
    "glob": "^10.3.10",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "less": "^4.1.3",
    "lint-staged": "^15.0.2",
    "np": "^10.0.5",
    "prettier": "^3.1.0",
    "rc-select": "^14.1.2",
    "rc-test": "^7.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "exports": {
    ".": {
      "default": "./dist/es/index.js",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://react-component.github.io/pagination",
  "keywords": [
    "react",
    "react-component",
    "react-pagination",
    "pagination",
    "antd",
    "pager"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "name": "@bees-ui/pagination",
  "peerDependencies": {
    "preact": "^10.24.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/pagination.git"
  },
  "scripts": {
    "build": "pnpm bee build --minify --sourcemap",
    "clean": "pnpm rimraf node_modules .turbo dist",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "coverage": "rc-test --coverage",
    "dev": "pnpm bee build --watch --sourcemap",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "gh-pages": "npm run docs:build && npm run docs:deploy",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "now-build": "npm run docs:build",
    "prepare": "husky install",
    "prepublishOnly": "npm run compile && np --yolo --no-publish && npm run gh-pages",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "start": "dumi dev",
    "test": "rc-test"
  },
  "tnpm": {
    "mode": "npm"
  },
  "types": "./dist/types/index.d.ts",
  "version": "0.0.0"
}