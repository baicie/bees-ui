    "cheerio": "1.0.0-rc.12",
    "cross-env": "^7.0.0",
    "dumi": "^2.1.3",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react": "^7.20.6",
    "father": "^4.1.5",
    "gh-pages": "^6.1.1",
    "glob": "^11.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^15.2.0",
    "np": "^10.0.5",
    "prettier": "^3.2.1",
    "rc-drawer": "^7.0.0",
    "rc-select": "^14.11.0",
