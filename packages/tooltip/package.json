  "description": "copy from antd",
  "devDependencies": {
    "@rc-component/father-plugin": "^1.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/warning": "^3.0.0",
    "cross-env": "^7.0.0",
    "dumi": "^2.2.13",
    "eslint": "^8.56.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "father": "^4.0.0",
    "gh-pages": "^3.1.0",
    "less": "^4.1.1",
    "np": "^7.1.0",
    "rc-test": "^7.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.0.5"
  },
  "exports": {
    ".": {
      "default": "./dist/es/index.js",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "http://github.com/react-component/tooltip",
  "keywords": [
    "react",
    "react-component",
    "react-tooltip",
    "tooltip"
  ],
  "license": "MIT",
  "main": "./dist/lib/index.js",
  "maintainers": [
    "yiminghe@gmail.com"
  ],
  "module": "./dist/es/index.js",
  "name": "@bees-ui/tooltip",
  "peerDependencies": {
    "preact": "^10.24.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/tooltip.git"
  },
  "scripts": {
    "build": "pnpm bee build --minify --sourcemap",
    "clean": "pnpm rimraf node_modules .turbo dist",
    "compile": "father build && lessc assets/bootstrap.less assets/bootstrap.css && lessc assets/bootstrap_white.less assets/bootstrap_white.css",
    "dev": "pnpm bee build --watch --sourcemap",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d dist",
    "lint": "eslint src/ --ext .tsx,.ts,.jsx,.js",
    "now-build": "npm run docs:build",
    "postpublish": "npm run docs:build && npm run docs:deploy",
    "prepare": "dumi setup",
    "prepublishOnly": "npm run compile && np --no-cleanup --yolo --no-publish",
    "start": "dumi dev",
    "test": "rc-test"
  },
  "types": "./dist/types/index.d.ts",
  "version": "0.0.0"
}