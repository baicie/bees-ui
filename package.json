{
  "name": "ui",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks && pnpm run stub",
    "biome": "biome migrate eslint --write --include-inspired",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "typecheck": "echo typecheck",
    "clean": "turbo run clean && rimraf node_modules .turbo",
    "stub": "turbo run stub",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test-unit": "echo test-unit",
    "test-e2e": "echo test-e2e",
    "test-docs": "pnpm run docs-build",
    "test": "run-s test-unit test-e2e test-docs",
    "docs": "pnpm --filter=docs run docs",
    "docs-build": "pnpm --filter=docs run docs-build",
    "docs-serve": "pnpm --filter=docs run docs-serve",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "update-version": "tsx scripts/update-version.ts",
    "publish-script": "tsx scripts/publish-script.ts",
    "update-deps": "ncu -uws -x eslint && ncu -u -x eslint",
    "patch-ants": "tsx ./scripts/patchReactToPreact.ts",
    "check-deps": "depcheck"
  },
  "keywords": [],
  "author": "baicie",
  "license": "MIT",
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "devDependencies": {
    "@ant-design/colors": "^7.1.0",
    "@ant-design/cssinjs": "1.21.0",
    "@ant-design/icons": "^5.4.0",
    "@bees-ui/internal-build": "workspace:^",
    "@bees-ui/register": "workspace:^",
    "@biomejs/biome": "^1.8.3",
    "@ctrl/tinycolor": "^4.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/plugin-commands-patching": "^6.2.3",
    "@preact/preset-vite": "^2.9.0",
    "@swc/core": "^1.7.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/stylis": "^4.2.6",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@umijs/fabric": "^4.0.1",
    "adm-zip": "^0.5.14",
    "antd": "^5.19.3",
    "chalk": "^5.3.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "consola": "^3.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-compat": "^6.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-x": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.1.1",
    "git-clone": "^0.2.0",
    "globals": "^15.8.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.2",
    "ora": "^8.0.1",
    "picocolors": "^1.0.1",
    "preact": "^10.22.1",
    "preact-custom-element": "^4.3.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "rc-field-form": "^2.2.1",
    "rc-motion": "^2.9.2",
    "rc-pagination": "^4.2.0",
    "rc-picker": "^4.6.9",
    "rc-resize-observer": "^1.4.0",
    "rc-tooltip": "^6.2.0",
    "rc-util": "^5.43.0",
    "react": "^18.3.1",
    "request": "^2.88.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-swc3": "^0.11.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.1",
    "tsx": "^4.16.2",
    "turbo": "^2.0.9",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "unbuild": "^2.0.0",
    "vite": "^5.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "biome format --write .",
      "biome lint ."
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "@ant-design/cssinjs@1.21.0": "patches/@ant-design__cssinjs@1.21.0.patch",
      "@ant-design/icons@5.4.0": "patches/@ant-design__icons@5.4.0.patch",
      "rc-motion@2.9.2": "patches/rc-motion@2.9.2.patch",
      "rc-pagination@4.2.0": "patches/rc-pagination@4.2.0.patch",
      "rc-picker@4.6.9": "patches/rc-picker@4.6.9.patch",
      "rc-util@5.43.0": "patches/rc-util@5.43.0.patch",
      "rc-field-form@2.2.1": "patches/rc-field-form@2.2.1.patch",
      "rc-resize-observer@1.4.0": "patches/rc-resize-observer@1.4.0.patch",
      "rc-tooltip@6.2.0": "patches/rc-tooltip@6.2.0.patch",
      "antd@5.19.3": "patches/antd@5.19.3.patch"
    }
  },
  "ncu": {
    "reject": [
      "eslint"
    ]
  }
}