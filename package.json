{
  "name": "ui",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "biome:format": "biome format --write .",
    "cz": "bcz",
    "update-version": "tsx scripts/update-version.ts",
    "publish-script": "tsx scripts/publish-script.ts",
    "prepare": "is-ci || husky install",
    "clean": "turbo run clean && pnpm run clean-root",
    "clean-root": "rimraf node_modules .turbo .stencil",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "stub": "turbo run stub",
    "postinstall": "pnpm run stub",
    "base": "turbo run base",
    "fix": "turbo run lint-fix",
    "lint": "turbo run lint",
    "prettier": "turbo run prettier",
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev",
    "core-dev": "turbo run core-dev",
    "tools-dev": "turbo run tools-dev",
    "update": "ncu -uws"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "biome format --write",
    "*.{json,less,md}": "prettier --ignore-unknown --write"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.4.0",
  "devDependencies": {
    "@baicie/commitizen": "^0.1.0",
    "@bees-ui/build": "workspace:^",
    "@biomejs/biome": "^1.8.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.10",
    "@types/stylis": "^4.2.6",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@umijs/fabric": "^4.0.1",
    "antd": "^5.19.1",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-compat": "^6.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-solid": "^0.14.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "turbo": "^2.0.6",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0"
  }
