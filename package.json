{
  "name": "ui",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks && pnpm run stub",
    "biome": "biome migrate eslint --write --include-inspired",
    "format": "biome format --write .",
    "fix": "biome lint --write .",
    "lint": "biome lint .",
    "typecheck": "echo typecheck",
    "clean": "turbo run clean && rimraf node_modules .turbo",
    "stub": "turbo run stub",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test-sc-unit": "vitest run -c ./packages/vite.config.ts",
    "test-unit": "vitest run",
    "test-e2e": "echo test-e2e",
    "test-docs": "pnpm run docs-build",
    "test": "run-s test-unit test-e2e test-docs",
    "docs": "pnpm --filter=docs run docs",
    "docs-build": "pnpm --filter=docs run docs-build",
    "docs-serve": "pnpm --filter=docs run docs-serve",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "update-version": "tsx scripts/update-version.ts",
    "publish-script": "tsx scripts/publish-script.ts",
    "update": "ncu -uws -x eslint && ncu -u -x eslint",
    "temp": "pnpm bee template"
  },
  "keywords": [],
  "author": "baicie",
  "license": "MIT",
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "devDependencies": {
    "@bees-ui/internal-cli": "workspace:^",
    "@biomejs/biome": "^1.8.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@solidjs/testing-library": "^0.8.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.4",
    "@types/stylis": "^4.2.6",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@umijs/fabric": "^4.0.1",
    "chalk": "^5.3.0",
    "clsx": "^2.1.1",
    "consola": "^3.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-compat": "^6.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-import-x": "^4.2.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "is-ci": "^3.0.1",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "solid-js": "^1.8.22",
    "tsup": "8.2.4",
    "tsx": "^4.19.0",
    "turbo": "^2.1.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "unbuild": "^2.0.0",
    "vite": "^5.4.3",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "biome format --write .",
      "biome lint ."
    ]
  }
}
