{
  "name": "ui",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks && pnpm run stub",
    "biome": "biome migrate eslint --write --include-inspired",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "typecheck": "echo typecheck",
    "clean": "turbo run clean && rimraf node_modules .turbo --turbo",
    "stub": "turbo run stub",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test-unit": "echo test-unit",
    "test-e2e": "echo test-e2e",
    "test-docs": "pnpm run docs-build",
    "test": "run-s test-unit test-e2e test-docs",
    "docs": "pnpm --filter=docs run docs",
    "docs-build": "pnpm --filter=docs run docs-build",
    "docs-serve": "pnpm --filter=docs run docs-serve",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "update-version": "tsx scripts/update-version.ts",
    "publish-script": "tsx scripts/publish-script.ts",
    "update": "ncu -uws && ncu -u"
  },
  "keywords": [],
  "author": "baicie",
  "license": "MIT",
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "devDependencies": {
    "@bees-ui/internal-build": "workspace:^",
    "@biomejs/biome": "^1.8.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.10",
    "@types/stylis": "^4.2.6",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@umijs/fabric": "^4.0.1",
    "antd": "^5.19.2",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-compat": "^6.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-x": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-solid": "^0.14.1",
    "eslint-plugin-unicorn": "^54.0.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.8.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "turbo": "^2.0.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "unbuild": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "biome format --write .",
      "biome lint ."
    ]
  }
}
