{
  "name": "@bees-ui/sc-element",
  "version": "0.0.0",
  "description": "Webcomponents wrapper for Solid",
  "lsicense": "MIT",
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/es/index.js",
      "require": "./dist/lib/index.js",
      "import": "./dist/es/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm bee build --minify --sourcemap",
    "dev": "pnpm bee build --watch --sourcemap",
    "sc-dev": "pnpm bee build --watch --sourcemap",
    "lint-fix": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md --fix",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm rimraf node_modules .turbo dist"
  },
  "dependencies": {
    "component-register": "~0.8.5"
  },
  "peerDependencies": {
    "solid-js": "^1.8.18"
  },
  "devDependencies": {
    "solid-js": "^1.8.18"
  }
}
