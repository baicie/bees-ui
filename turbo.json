{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "base": {
      "cache": true
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "core-dev": {
      "cache": true
    },
    "tools-dev": {
      "cache": true
    },
    "test": {},
    "lint-fix": {},
    "lint": {},
    "prettier": {},
    "interbal-stub": {},
    "stub": {
      "cache": true,
      "dependsOn": ["^stub"]
    }
  },
  "ui": "stream"
}
