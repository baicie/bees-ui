{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "base": {
      "cache": true
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": true
    },
    "test": {},
    "lint": {},
    "stub": {
      "cache": true
    }
  }
}